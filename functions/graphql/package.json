{
  "scripts": {
    "bundle": "npm run makeCache && npm run zip",
    "webpack": "webpack",
    "zip": "find . -type f  -not -name '*.js.map' -not -wholename '*/.git/*' -not -wholename '*/node_modules/.cache/*' -not -wholename '*/node_modules/webpack-cli/*' -not -wholename '*/node_modules/webpack/*' -not -wholename '*/node_modules/rxjs/*' -not -wholename '*/node_modules/prettier/*' -not -wholename '*/node_modules/fsevents/*' -not -wholename '*/node_modules/@babel/*' -not -wholename '*/node_modules/webpack-addons/*' -not -wholename '*/node_modules/caniuse-lite/*' | zip -Xr@ conversation.zip",
    "makeCache": "node makeCache",
    "local": "export $(grep -v '^#' .env.local | xargs) && npm run makeCache && node local"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "4.16.2",
    "pg": "7.4.1",
    "postgraphile": "^4.3.3",
    "raven": "^2.6.4",
    "serverless-http": "^2.3.0"
  }
}
